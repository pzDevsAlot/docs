            "pages": [
              "pzvip-getting-started",
              "pzvip-Configuration",
              {
                "group": "Exports",
                "expanded": true,
                "pages": [
                  "pzvip-client",
                  "pzvip-server"
                ]
              },
              "pzvip-commands-guide"
            ]
          },
          {
            "group": "pz_antimapcollision",
            "expanded": true,
            "pages": [
              "pzantimap-getting-started",
              "pzantimap-Configuration",
              "pzantimap-common-issues"
            ]
          }
        ]
      }
    ]
  },
  "navbar": {
    "links": [
      {
        "label": "pz Support",
        "href": "https://discord.gg/yXPUBgZUmb"
      }
    ],
    "primary": {
      "type": "button",
